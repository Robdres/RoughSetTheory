    "import numpy as np\n",
    "def Circle(r):\n",
    "    dia = r*2\n",
    "    area = r**2 * np.pi\n",
    "    per = dia*np.pi\n",
    "    print(\"Diametro =\",dia,\" Area=\",area,\" Perimetro=\",per)\n",
    "    return [dia,area,per]"
   ]
